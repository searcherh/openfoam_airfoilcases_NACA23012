FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

Ux 15.99025;
Uy 0.55839;

internalField   uniform ($Ux $Uy 0);

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    farfield
    {
        type            freestreamVelocity;
        freestreamValue $internalField;
    }
    airfoil
    {
        type            noSlip;
    }
}

// ************************************************************************* //
